{
  "name": "ssm-web5",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host --open",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint --fix . && stylelint --fix \"**/*.{svelte,stylus}\"",
    "format": "prettier --plugin-search-dir . --write .",
    "microcms:typegen": "microcms-typescript src/lib/microcms/schema src/lib/microcms/types.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "microcms-typescript": "^1.0.14",
    "prettier": "^2.8.1",
    "prettier-plugin-stylus": "0.0.1-beta.8",
    "prettier-plugin-svelte": "^2.9.0",
    "stylelint": "^14.16.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-stylus": "^0.17.0",
    "stylus": "^0.59.0",
    "svelte": "^3.55.0",
    "svelte-check": "^2.10.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.1"
  },
  "type": "module",
  "dependencies": {
    "@splidejs/svelte-splide": "^0.2.9",
    "cdate": "^0.0.4",
    "microcms-js-sdk": "^2.3.2"
  }
}
